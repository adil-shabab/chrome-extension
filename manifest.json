{
    "manifest_version": 3,
    "version": "2.8",
    "name": "Tab Saver",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon16.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs"
    ]
}